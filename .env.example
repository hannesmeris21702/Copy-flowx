JSON_RPC_ENDPOINT=https://fullnode.mainnet.sui.io:443

LOG_LEVEL=info

### Worker options
PROTOCOL=CETUS
TARGET_POOL=0x88cec280ed5406af7951ef768b305de5323b843cc127bcab988d08770d00a5f7

# Strategy mode: SAFE or AGGRESSIVE
STRATEGY_MODE=SAFE

# Price range configuration (in basis points, 10000 = 100%)
BPRICE_PERCENT=50                     # Bottom price % (default: 50 = 0.5%)
TPRICE_PERCENT=10                     # Top price % (default: 10 = 0.1%)

# Trading parameters
SLIPPAGE_TOLERANCE=50000              # 5% slippage tolerance
PRICE_IMPACT_PERCENT_THRESHOLD=-5000  # -0.5% max price impact

# Minimum amounts for operations
MIN_ZAP_AMOUNT_X=10000
MIN_ZAP_AMOUNT_Y=10000

# Position size multiplier
MULTIPLIER=1

# Reward compounding configuration
REWARD_THRESHOLD_USD=1                # Minimum USD value of rewards to collect
COMPOUND_REWARDS_SCHEDULE_MS=3600000  # 1 hour in milliseconds

# Risk management
REBALANCE_COOLDOWN_MS=3600000         # 1 hour cooldown between rebalances
MAX_VOLATILITY_PERCENT=1000           # 10% max volatility (in basis points)

# Event monitoring
LARGE_SWAP_THRESHOLD_USD=100000       # $100k threshold for large swap detection

# Optional: Address for tracking volume
TRACKING_VOLUME_ADDRESS=

# Retry configuration
REBALANCE_RETRIES=3

# REQUIRED: Private key for signing transactions (without 0x prefix)
PRIVATE_KEY=
